      <link name="${prefix}_link">
        <visual>
          <origin xyz="${v_xyz}" rpy="${v_rpy}"/>
          <geometry>
            <mesh filename="file://$(find hexapod_model_description)/meshes/${prefix}_link.stl" scale="0.001 0.001 0.001"/>
          </geometry>
          <material name="gray">
            <color rgba="0.2 0.2 0.2 1"/>
          </material>
        </visual>
        <!-- Collision model using simplified shapes -->
        <collision>
          <origin xyz="${c_xyz}" rpy="${c_rpy}"/>
          <geometry>
            <mesh filename="file://$(find hexapod_model_description)/meshes/${prefix}_link.stl" scale="0.001 0.001 0.001"/>
          </geometry>
        </collision>
        <inertial>
          <origin xyz="${i_xyz}" rpy="${i_rpy}"/>
          <mass value="${mass}"/>
          <inertia ixx="${ixx}" iyy="${iyy}" izz="${izz}" ixy="${ixy}" iyz="${iyz}" ixz="${ixz}"/>
        </inertial>
      </link>
    </xacro:macro>

